        <class_source>
import java.security.*;
import javax.crypto.*;
import javax.crypto.spec.*;
import org.apache.commons.codec.binary.Hex;

private int cryptographyKeySize=0;
private String cryptographyMethod=null;
private	KeyGenerator kgen = null;
private int secretKeyFieldIndex=0;
private int ivVectorIndex=0;
private int saltIndex=0;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException 
{

	// there are no rows coming in, we are generating one row with the key

	if (first) {

		cryptographyMethod=getVariable("cryptographyMethod", "");
		cryptographyKeySize=Const.toInt(getVariable("cryptographyKeySize", ""),0);
		try {
			kgen = KeyGenerator.getInstance(cryptographyMethod);
			kgen.init(cryptographyKeySize); // 192 and 256 bits may not be available (see notes)
		} catch (Exception e) {
			throw new KettleStepException(e);
		}

		// output field
		secretKeyFieldIndex = data.outputRowMeta.indexOfValue("secretKey");
		if (secretKeyFieldIndex&lt;0) {
			throw new KettleException("'secretKey' field not found in the output row, check Fields and add the fieldname!");
		}
		ivVectorIndex = data.outputRowMeta.indexOfValue("ivVector");
		if (ivVectorIndex&lt;0) {
			throw new KettleException("'ivVector' field not found in the output row, check Fields and add the fieldname!");
		}
		saltIndex = data.outputRowMeta.indexOfValue("salt");
		if (saltIndex&lt;0) {
			throw new KettleException("'salt' field not found in the output row, check Fields and add the fieldname!");
		}

		first=false;
 	}
 
	Object[] outputRowData = createOutputRow(null, data.outputRowMeta.size());

	// Generate the secret key specs.
	SecretKey skey = kgen.generateKey();
    byte[] raw = skey.getEncoded();

	outputRowData[secretKeyFieldIndex]=new String(Hex.encodeHex(raw));

	// Generate the salt key
	skey = kgen.generateKey();
    raw = skey.getEncoded();

	String alphabet = "abcdefghijklmnopqrstuvwxyz" + "ABCDEFGHIJKLMNOPQRSTUVWXYZ" + "01234567890" + "$#_";
    final int N = alphabet.length();
    SecureRandom r = new SecureRandom();
    String finalStr= "";
    for (int i = 0; i &lt; 50; i++) {
       finalStr+=alphabet.charAt(r.nextInt(N));
    }
	outputRowData[saltIndex]=new String(Hex.encodeHex(finalStr.getBytes()));

	finalStr= "";
    for (int i = 0; i &lt; 8; i++) {
       finalStr+=alphabet.charAt(r.nextInt(N));
    }
	outputRowData[ivVectorIndex]=new String(Hex.encodeHex(finalStr.getBytes()));
	putRow(data.outputRowMeta, outputRowData);

	setOutputDone();
	return false;
}

