    <definitions>
      <definition>
        <class_type>TRANSFORM_CLASS</class_type>
        <class_name>Processor</class_name>
        <class_source>import java.util.Base64;
import java.security.*;
import javax.crypto.*;
import javax.crypto.spec.*;
import org.apache.commons.codec.binary.Hex;

private int cryptographyKeySize=0;
private String cryptographyMethod=null;
private	KeyGenerator kgen = null;
private int secretKeyFieldIndex=0;
private int encryptedTextFieldIndex=0;
private int ivVectorFieldIndex=0;
private int myDecryptedSecretFieldIndex=0;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException 
{
	Object[] r = getRow(); 

	if (r==null)
	{
		setOutputDone();
		return false;
	}

	if (first) {

		cryptographyMethod=getVariable("cryptographyMethod", "");
		cryptographyKeySize=Const.toInt(getVariable("cryptographyKeySize", ""),0);

		// input fields
		secretKeyFieldIndex = getInputRowMeta().indexOfValue(getParameter("secretKeyField"));
		if (secretKeyFieldIndex&lt;0) {
			throw new KettleException(getParameter("secretKeyField")+" field not found in the input row, check parameter 'secretKeyField'!");
		}
		encryptedTextFieldIndex = getInputRowMeta().indexOfValue(getParameter("encryptedTextField"));
		if (encryptedTextFieldIndex&lt;0) {
			throw new KettleException(getParameter("encryptedTextField")+" field not found in the input row, check parameter 'encryptedTextField'!");
		}
		ivVectorFieldIndex = getInputRowMeta().indexOfValue(getParameter("ivVectorField"));
		if (ivVectorFieldIndex&lt;0) {
			throw new KettleException(getParameter("ivVectorField")+" field not found in the input row, check parameter 'ivVectorField'!");
		}

		// output fields
		myDecryptedSecretFieldIndex = data.outputRowMeta.indexOfValue("myDecryptedSecret");
		if (myDecryptedSecretFieldIndex&lt;0) {
			throw new KettleException("'myDecryptedSecret' field not found in the output row, check Fields and add the fieldname!");
		}

		first=false;
	}
 
	Object[] outputRowData = createOutputRow(r, data.outputRowMeta.size());

	// keep it simple, assume the key is available in each row
	byte[] raw = null;
	byte[] ivs = null;
	SecretKeySpec skeySpec = null;
	IvParameterSpec iv;
	try {
		raw = Hex.decodeHex(((String)r[secretKeyFieldIndex]).toCharArray());
		skeySpec = new SecretKeySpec(raw, "DESede");
	} catch (Exception e) {
		throw new KettleStepException(e);
	}
	// get IV vector
	try {
		ivs = Hex.decodeHex(((String)r[ivVectorFieldIndex]).toCharArray());
		iv = new IvParameterSpec(ivs);
	} catch (Exception e) {
		throw new KettleStepException(e);
	}

	// Instantiate the cipher (could be optimized and put into the "first" section when the key is always the same)
	Cipher cipher = null;
	try {
		cipher = Cipher.getInstance(cryptographyMethod);
		cipher.init(Cipher.DECRYPT_MODE, skeySpec, iv);
	} catch (Exception e) {
		throw new KettleStepException(e);
	}

	byte[] decrypted = null;
	try {
	   decrypted = cipher.doFinal(Base64.getDecoder().decode(((String)r[encryptedTextFieldIndex]).getBytes()));
	} catch (Exception e) {
		throw new KettleStepException(e);
	}

	outputRowData[myDecryptedSecretFieldIndex]=new String(decrypted);

	putRow(data.outputRowMeta, outputRowData);

	return true;
}
