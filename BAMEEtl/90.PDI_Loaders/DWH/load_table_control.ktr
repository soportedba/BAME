    <quoteString>N</quoteString>
    <sql>CREATE TABLE if not exists mtdt_load_control
(
  id bigserial NOT NULL,
  ID_CUBE numeric,
  fec_ini timestamp(3) without time zone NOT NULL, 
  fec_fin timestamp(3) without time zone NOT NULL, 
  completed character varying(1) NOT NULL, 
   PARENT_ID BIGINT,
   NUM_ROWS BIGINT,
  CONSTRAINT mtdt_load_table_pkey PRIMARY KEY (id),
  CONSTRAINT mtdt_load_table_completada_check CHECK (completed::text = ANY (ARRAY['N'::character varying, 'Y'::character varying,'P'::character varying]::text[])),
  CONSTRAINT FK_MTDT_LOAD_CONTROL FOREIGN KEY (PARENT_ID) REFERENCES MTDT_LOAD_CONTROL (ID) 
)
WITH (
  OIDS=FALSE
);
-- Include a gap time margin (interval 2 hours) to prevent different timestamp non-sync errors
insert into mtdt_load_control (fec_ini, fec_fin, completed, num_rows, id_cube) 
(select coalesce(fec_fin,now()-interval '100 year'), now()-interval '? minutes', 'N', 0,?  FROM 
	(select max(fec_fin) fec_fin from mtdt_load_control where upper(completed)='Y' and id_cube=?) a
where 0=(select count(1) from mtdt_load_control where upper(coalesce(completed,'N')) ='N' and id_cube=?)
);</sql>
