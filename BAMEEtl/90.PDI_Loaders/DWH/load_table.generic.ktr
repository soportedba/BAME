    <definitions>
      <definition>
        <class_type>TRANSFORM_CLASS</class_type>
        <class_name>Processor</class_name>
        <class_source>import java.math.*;
import java.util.*;
import java.util.Map.Entry;
import com.mongodb.MongoClient;
import com.mongodb.MongoClientURI;
import com.mongodb.DB;
import com.mongodb.DBCollection;
import com.mongodb.DBCursor;
import com.mongodb.DBObject;
import com.mongodb.util.JSON;

private DB db;
private DBCollection coll;
private int outputRowSize = 0;
private MongoClient m = null;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
  Object[] r=getRow();
  if (r==null)
  {
    setOutputDone();
	return false;
  }


	String database= get(Fields.In, "database").getString(r);
	String ipport= get(Fields.In, "ipport").getString(r);
	String username= get(Fields.In, "username").getString(r);
	String password= get(Fields.In, "password").getString(r);
	String shard= get(Fields.In, "shard").getString(r);
	String collection=get(Fields.In, "dmlquery").getString(r);

	try {
	String replset="";
	if (!shard.equals("undefined") &amp;&amp; !shard.equals("") &amp;&amp; !shard.equals("none")){
		replset="&amp;replicaSet="+shard;
	}
	String mongolabUri = "mongodb://"+username+':'+password+'@'+ipport+'/'+database+"?connectTimeoutMS=3600000&amp;socketTimeoutMS=3600000&amp;maxIdleTimeMS=300000&amp;readPreference=secondaryPreferred"+replset;
	//logBasic(mongolabUri);
	MongoClientURI uri = new MongoClientURI(mongolabUri);

	m = new MongoClient(uri);
		db = m.getDB( database );
    		coll = db.getCollection(collection );

	} catch(Exception e) {
	  	logError("Error connecting to MongoDB: ", e);
    		return false;
	}

	Integer f_skip=((Long) get(Fields.In, "f_skip").getInteger(r)).intValue();
	Integer f_limit=((Long) get(Fields.In, "f_limit").getInteger(r)).intValue();
	String source_timestamp_column = get(Fields.In, "source_timestamp_column").getString(r);
	String FEC_INI_ISO = get(Fields.In, "FEC_INI_ISO").getString(r);
	String FEC_FIN_ISO = get(Fields.In, "FEC_FIN_ISO").getString(r);
	String filter = "";

	if (FEC_INI_ISO.equals("1900-01-01T00:00:00.000Z") &amp;&amp; FEC_INI_ISO.equals("2200-01-01T00:00:00.000Z")) {
		filter = "{}";
	}else{
		filter = "{\""+source_timestamp_column+"\":{$gte:{$date:\""+FEC_INI_ISO+"\"},$lte:{$date:\""+FEC_FIN_ISO+"\"}}}";
	}

	String proyect="{}";

	DBObject jsonFilter = (DBObject) JSON.parse(filter);
	DBObject jsonProyect = (DBObject) JSON.parse(proyect);
	
	DBCursor cur = coll.find(jsonFilter,jsonProyect).skip(f_skip).limit(f_limit);
	//DBCursor cur = coll.find(jsonFilter,jsonProyect);
	
	if (first) {
		first=false;
		outputRowSize = data.outputRowMeta.size();
 	}
	while(cur.hasNext() &amp;&amp; !isStopped()) {
		String json = cur.next().toString();

		Object[] outputRow = createOutputRow(new Object[0], data.outputRowMeta.size());

		get(Fields.Out, "json_collection").setValue(outputRow, json);
		get(Fields.Out, "fec_ini_iso").setValue(outputRow, FEC_INI_ISO);
		get(Fields.Out, "fec_fin_iso").setValue(outputRow, FEC_FIN_ISO);
		putRow(data.outputRowMeta, outputRow);
	}
   	return true;
}

public boolean init(StepMetaInterface stepMetaInterface, StepDataInterface stepDataInterface)
{
	return parent.initImpl(stepMetaInterface, stepDataInterface);
}

public void dispose(StepMetaInterface smi, StepDataInterface sdi)
{
    if (m != null) {
        m.close();
    }
 
    parent.disposeImpl(smi, sdi);
}
